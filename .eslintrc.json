{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "strict": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/destructuring-assignment":"off",
      "react/button-has-type": "off",
      "no-alert":"off",
      "react/no-danger" : "off",
      "react/forbid-prop-types": "off"
    },
    "env": {
        "browser": true,
        "node": true,
        "jest":true
    }

}
